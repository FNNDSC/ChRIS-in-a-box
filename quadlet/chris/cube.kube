[Install]
WantedBy=default.target

[Unit]
Requires=db.service
After=db.service
Requires=swift.service
After=swift.service
Requires=rabbitmq.service
After=rabbitmq.service

[Kube]
# Point to the yaml file in the same directory
Yaml=cube.yml

# Use the Chris network
Network=chris.network

# Use the envoy proxy config map in the same directory
ConfigMap=minichris-cube-config.yml
ConfigMap=minichris-db-config.yml
ConfigMap=minichris-docker-swift-onlyone-config.yml
