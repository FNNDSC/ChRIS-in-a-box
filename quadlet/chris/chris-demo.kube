[Install]
WantedBy=default.target

[Unit]
Requires=quadlet-demo-mysql.service
After=chris-demo-mysql.service

[Kube]
# Point to the yaml file in the same directory
Yaml=quadlet-demo.yml
# Use the chris-demo network
Network=chris-demo.network
# Use the config map in the same directory
ConfigMap=configmap.yml

